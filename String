public class Main
{
	public static void main(String[] args) {
	    String name;
	    String country;
	    String sex;
	    double height;
	    int age;
	    name = "kiran";
	    country = "singapore";
	    sex = "m";
	    height = 1.70;
	    age = 33;
		System.out.println("name "+name);
		System.out.println("country "+country);
		System.out.println("gender "+sex);
		System.out.println("height "+height);
		System.out.println("age "+age);
	}
}
